<%- include(`partials/header`) -%>

    <head>
        <title>Book With Us</title>
        <!-- Custom styles for this template -->
        <link href="about_style.css" rel="stylesheet">
    </head>
    <%if(state ===`navbar_unlogged`){ %>
    <%- include(`partials/navbar_unlogged`) -%>
    <%}else if(state===`navbar_logged`){%>
    <%- include(`partials/navbar_logged`,{user:userID}) -%>
    <%}else{%>
    <%-include(`partials/navbar_logged_admin`,{user:userID}) -%>
    <%}%>
    <main class="text-light">
        <!-- Nav tabs -->
        <nav class="d-flex justify-content-center mb-3">
            <ul class="nav nav-tabs" id="myTab" role="tablist">
                <li class="nav-item flex-grow-1 bg-white" role="presentation">
                    <button class="nav-link active" id="about-tab" data-bs-toggle="tab" data-bs-target="#about"
                        type="button" role="tab" aria-controls="about" aria-selected="true">About Us</button>
                </li>
                <li class="nav-item flex-grow-1 bg-white" role="presentation">
                    <button class="nav-link" id="route-tab" data-bs-toggle="tab" data-bs-target="#route" type="button"
                        role="tab" aria-controls="route" aria-selected="false">Routes</button>
                </li>
                <li class="nav-item flex-grow-1 bg-white" role="presentation">
                    <button class="nav-link" id="contact-tab" data-bs-toggle="tab" data-bs-target="#contact"
                        type="button" role="tab" aria-controls="contact" aria-selected="false">Contact</button>
                </li>
            </ul>
        </nav>

        <!-- Tab panes -->
        <section class="tab-content">
            <div class="tab-pane fade active show" id="about" role="tabpanel" aria-labelledby="about-tab">
                <div class="container background-bus rounded-3">
                    <div class="d-flex justify-content-around">
                        <div class="col-md-6 ps-3 me-3">
                            <a class="display-1 text-decoration-none text-black bg rounded-2 ps-0 pe-2"
                                href="/booking"><b class="text-secondary ms-0 ps-0">Book With Us</b></a>
                            <p class="fs-5 pt-3 pe-5 ps-1">No more waiting, go ahead and booking on the IIITDMJ Bus
                                Booking
                                System.
                        </div>
                        <div class="col-md-6">
                            <p class="fs-5">IIITDMJ has launched it's Bus System with exclusive features. A multi-modal
                                transportation
                                service backed with technology. Bus System powered by IIITDMJ, always updates features
                                that
                                simplify travel for students.<br />
                                <br />1. Bus information with live GPS tracking for arrival and departure
                                confirmation.<br />
                                <br />2. Safety promise - With continuous CCTV and GPS tracking on buses,
                                round-the-clock
                                command
                                centre keeps a close watch for safe journey.<br />
                                <br />3. A special feature for women only seats.<br />
                            </p>
                        </div>
                    </div>
                </div>
            </div>
            <div class="tab-pane fade justify-content-evenly" id="route" role="tabpanel" aria-labelledby="route-tab">
                <nav class="d-flex justify-content-around mb-5">
                    <ul class="nav nav-pills" id="myTab" role="tablist">
                        <li class="nav-item flex-grow-1" role="presentation">
                            <button class="nav-link active text-white" id="contractualBus1-tab" data-bs-toggle="tab"
                                data-bs-target="#contractualBus1" type="button" role="tab"
                                aria-controls="contractualBus1" aria-selected="true">Contractual Bus 1</button>
                        </li>
                        <li class="nav-item flex-grow-1" role="presentation">
                            <button class="nav-link text-white" id="contractualBus2-tab" data-bs-toggle="tab"
                                data-bs-target="#contractualBus2" type="button" role="tab"
                                aria-controls="contractualBus2" aria-selected="false">Contractual Bus 2</button>
                        </li>
                        <li class="nav-item flex-grow-1" role="presentation">
                            <button class="nav-link text-white" id="instituteBus-tab" data-bs-toggle="tab"
                                data-bs-target="#instituteBus" type="button" role="tab" aria-controls="instituteBus"
                                aria-selected="false">Institute Bus</button>
                        </li>
                    </ul>
                </nav>
                <section class="tab-content">
                    <div class="tab-pane fade active show" id="contractualBus1" role="tabpanel"
