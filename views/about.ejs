                        <div class="container">
                            <div class="d-flex justify-content-evenly ps-3 pe-3">
                                <div class="col-md-10 col-lg-8 col-xl-6 col-sm-12">
                                    <p class="display-5 pt-0 p-2 fw-light text-center">Monday - Friday</p> 
                                    <div class="row ps-4 pb-2 pt-2 bg-dark text-white rounded-top rouned-3 bg-gradient">
                                        <div class="col">
                                            From
                                        </div>
                                        <div class="col">
                                            To
                                        </div>
                                        <div class="col">
                                            Out Time
                                        </div>
                                        <div class="col">
                                            Purpose
                                        </div>
                                    </div>
                                    <% q1_res.forEach((element,index) => { %>
                                        <%   if(index%2===0){ %> 
                                    <div class="row pt-2 pb-2 ps-4 bg-light bg-gradient text-black">
                                        <div class="col">
                                            <%= element.src %>
                                        </div>
                                        <div class="col">
                                            <%= element.des %>
                                        </div>
                                        <div class="col">
                                            <%= element.Out_Time %>
                                        </div>
                                        <div class="col">
                                            <%= element.purpose %>
                                        </div>
                                    </div>
                                    <% }else{ %>
